  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:development": "node $NODE_DEBUG_OPTION scripts/build.js --dev",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage",
    "server": "node scripts/server.js",
    "server:development": "nodemon $NODE_DEBUG_OPTION scripts/server.js --watch build/render-server.js",
    "bdd-test": "node ./node_modules/.bin/wdio wdio.conf.js",
    "eslint": "./node_modules/.bin/eslint .",
    "prettier": "./node_modules/.bin/prettier --print-width 120 --single-quote --no-semi --write \"apps/**/*.js\""
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "app/**/*.{js,jsx}",
      "server/**/*.{js,jsx}",
      "!src/**/index.js",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js",
      "^.+\\.scss$": "<rootDir>/config/jest/CSSStub.js"
    },
    "modulePaths": [
      "<rootDir>/apps/"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "raf/polyfill"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "transformIgnorePatterns": [
      "node_modules/(?!@gov.au)"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "engines": {
    "node": "8.2.1"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "public/",
      "name": "pancake.css"
    },
    "sass": {
      "modules": false,
      "location": "pancake/sass/",
      "name": "pancake.scss"
    },
    "js": {
      "minified": false,
      "modules": true,
      "location": "public/pancake/js/",
      "name": "pancake.min.js"
    },
    "react": {
      "location": "pancake/react/"
    }
  }
}
